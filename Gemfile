# Usage:
# Instllation: `$ bundle`

source 'https://rubygems.org'

gem 'jekyll-admin', '~> 0.3.0', group: :jekyll_plugins # Jekyll CMS dashboard
gem 'jekyll-paginate' # Pagination
gem 'jekyll-livereload', group: :jekyll_plugins # Live reload when making a change
gem 'jekyll-seo-tag' # Automatic SEO Tagging
gem 'jekyll-sitemap' # Automatic SEO sitemap generation
